{
  "author": "Lawrence Anthony Turton",
  "licens": "GPL-3.0-or-late",
  "bugs": {
    "url": "https://github.com/Modcat/blankcanvas/issues"
  },
  "build": {
    "appId": "com.blankcanvas.modcat",
    "asar": true,
    "directories": {
      "output": "build/"
    },
    "dmg": {
      "contents": [
        {
          "path": "/Applications",
          "type": "link",
          "x": 410,
          "y": 230
        },
        {
          "type": "file",
          "x": 130,
          "y": 230
        }
      ],
      "window": {
        "height": 380,
        "width": 540
      }
    },
    "files": [
      "dist/**/*",
      "_icons/icon.*"
    ],
    "linux": {
      "icon": "_icons/icon.png",
      "target": [
        "deb",
        "snap",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "_icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "type": "distribution"
    },
    "npmRebuild": true,
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "productName": "BlankCanvas",
    "win": {
      "icon": "_icons/icon.ico",
      "target": [
        "nsis",
        "zip",
        "portable",
        "squirrel"
      ]
    }
  },
  "dependencies": {
    "@handsontable/vue": "^4.1.0",
    "axios": "^0.18.0",
    "bulma-pro": "^0.1.6",
    "chalk": "^2.4.2",
    "codemirror": "^5.47.0",
    "electron-simple-publisher": "^0.6.2",
    "electron-simple-updater": "^1.5.0",
    "fs": "^0.0.1-security",
    "git": "^0.1.5",
    "handsontable": "^7.1.1",
    "hot-formula-parser": "^3.0.1",
    "node-zip": "^1.1.1",
    "open": "~>6.3.0",
    "perfect-scrollbar": "^1.4.0",
    "simple-git": "^1.126.0",
    "ts-interface-validator": "^1.1.0",
    "vue": "^2.6.10",
    "vue-chartist": "^2.2.0",
    "vue-drag-sortable": "^0.1.0",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-electron": "^1.0.3",
    "x-data-spreadsheet": "^1.0.27",
    "xterm": "^4.1.0"
  },
  "description": "Swiss knife application for developing in many different data types and environments with data connectivity supporting freedom computing for the application type.",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "devtron": "^1.4.0",
    "electron": "~5.0.1",
    "electron-builder": "^20.40.2",
    "electron-debug": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-vue": "^5.2.2",
    "fast-glob": "^2.2.7",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-loader": "^0.6.0",
    "prettier": "^1.17.1",
    "purgecss-webpack-plugin": "^1.5.0",
    "sass": "^1.20.1",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "tree-kill": "^1.2.1",
    "url-loader": "^1.1.2",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "license": "GNU GPL 2",
  "main": "./dist/main.js",
  "name": "blankcanvas",
  "repository": {
    "type": "git",
    "url": "https://github.com/Modcat/blankcanvas"
  },
  "scripts": {
    "build": "npm run pack && electron-builder",
    "dev": "node ./_scripts/dev-runner.js",
    "lint": "eslint --ext .js,.vue --fix src && prettier --write ./src/**/*.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "webpack --mode=production --env.NODE_ENV=production --hide-modules --config ./_scripts/webpack.main.config.js",
    "pack:renderer": "webpack --mode=production --env.NODE_ENV=production --hide-modules --config ./_scripts/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "version": "0.0.1"
}
